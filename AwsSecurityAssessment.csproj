<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <AssemblyName>AwsSecurityAssessment</AssemblyName>
    <RootNamespace>AwsSecurityAssessment</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
